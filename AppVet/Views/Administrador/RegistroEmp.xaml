<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppVet.Views.RegistroEmp"
             Background="#12a89d">
    <ContentPage>
        <StackLayout Background="#F5F7F7">
            <Image Source="adm.png"
               Margin="0,0,0,0"
              HeightRequest="200"
               WidthRequest="30"/>
            <Frame Background="#F5F7F7" Padding="2" CornerRadius="0">
                <Label Text="Registrar nuevo empleado " HorizontalTextAlignment="Center" TextColor="black" FontSize="20"/>
            </Frame>
            <Frame VerticalOptions="FillAndExpand"
               Margin="20,20,20,50"
               CornerRadius="20"
               Background="White">
                
                
            </Frame>
        </StackLayout>
    </ContentPage>
</MasterDetailPage>